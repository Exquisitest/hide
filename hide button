mouse = game.Players.LocalPlayer:GetMouse()
local ui = Instance.new("ScreenGui")
local Frame = Instance.new("Frame")
local tb = Instance.new("TextButton")

-- Prop.

ui.Name = "Hide"
ui.Parent = game.CoreGui
ui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling

Frame.Parent = ui
Frame.Position = UDim2.new(0.06, 0, 0, 0)
Frame.BackgroundColor3 = Color3.fromRGB(70, 70, 70)
Frame.Size = UDim2.new(0, 100, 0, 100)
Frame.BackgroundTransparency = 0.25
Frame.Draggable = false
Frame.Active = true

tb.Name = "TextButton_ui"
tb.Parent = Frame
tb.Position = UDim2.new(0.189999998, 0, 0.140000001, 0)
tb.Size = UDim2.new(0, 66, 0, 67)
tb.Text = "Hide"
tb.TextSize = 17.000
tb.BackgroundTransparency = 1
tb.TextColor3 = Color3.fromRGB(256, 256, 256)
tb.BorderSizePixel = 0

tb.MouseButton1Click:connect(function()
game.CoreGui.Cove:remove()
game.CoreGui.Facility:remove()
game.CoreGui.ArtifactA:remove()
game.CoreGui.ArtifactB:remove()
game.CoreGui.ArtifactC:remove()
game.CoreGui.ArtifactD:remove()
game.CoreGui.Watchtower:remove()
game.CoreGui.Hangar:remove()
game.CoreGui.Lotus:remove()
game.CoreGui.PRCSPAWN1:remove()
game.CoreGui.PRCSPAWN2:remove()
game.CoreGui.PRCSPAWN3:remove()
game.CoreGui.PRCSPAWN4:remove()
game.CoreGui.PRCSPAWN5:remove()
loadstring(game:HttpGet("https://raw.githubusercontent.com/Exquisitest/minimize/main/show%20button", true))()

wait(0.01)
game.CoreGui.Hide:remove()
end)
